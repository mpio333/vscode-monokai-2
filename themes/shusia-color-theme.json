// This theme's colors are based on the original Monokai:
//   #1e1f1c (tab well, borders)
//   #2d2a2e (editor background)
//   #403842 (selection)
//   #6f5e75 (focus)
//   #e3e1e4 (editor foreground)
{
  "name": "Monokai 2.0 - Shusia",
  "type": "dark",
  "colors": {
    "dropdown.background": "#403842",
    "list.activeSelectionBackground": "#6f5e75",
    "quickInputList.focusBackground": "#403842",
    "dropdown.listBackground": "#1e1f1c",
    "list.inactiveSelectionBackground": "#403842",
    "list.hoverBackground": "#3e3d32",
    "list.dropBackground": "#403842",
    "list.highlightForeground": "#e3e1e4",
    "button.background": "#6f5e75",
    "editor.background": "#2d2a2e",
    "editor.foreground": "#e3e1e4",
    "selection.background": "#878b9180",
    "editor.selectionHighlightBackground": "#575b6180",
    "editor.selectionBackground": "#878b9180",
    "minimap.selectionHighlight": "#878b9180",
    "editor.wordHighlightBackground": "#4a4a7680",
    "editor.wordHighlightStrongBackground": "#6a6a9680",
    "editor.lineHighlightBackground": "#3e3d32",
    "editorLineNumber.activeForeground": "#c2c2bf",
    "editorCursor.foreground": "#f8f8f0",
    "editorWhitespace.foreground": "#464741",
    "editorIndentGuide.background": "#464741",
    "editorIndentGuide.activeBackground": "#767771",
    "editorGroupHeader.tabsBackground": "#1e1f1c",
    "editorGroup.dropBackground": "#40384280",
    "tab.inactiveBackground": "#34352f",
    "tab.border": "#1e1f1c",
    "tab.inactiveForeground": "#ccccc7", // needs to be bright so it's readable when another editor group is focused
    "tab.lastPinnedBorder": "#403842",
    "widget.shadow": "#00000098",
    "progressBar.background": "#6f5e75",
    "badge.background": "#6f5e75",
    "badge.foreground": "#e3e1e4",
    "editorLineNumber.foreground": "#90908a",
    "panelTitle.activeForeground": "#e3e1e4",
    "panelTitle.activeBorder": "#6f5e75",
    "panelTitle.inactiveForeground": "#6f5e75",
    "panel.border": "#403842",
    "settings.focusedRowBackground": "#4038425A",
    "titleBar.activeBackground": "#1e1f1c",
    "statusBar.background": "#403842",
    "statusBar.noFolderBackground": "#403842",
    "statusBar.debuggingBackground": "#6f5e75",
    "statusBarItem.remoteBackground": "#AC6218",
    "ports.iconRunningProcessForeground": "#ccccc7",
    "activityBar.background": "#2d2a2e",
    "activityBar.foreground": "#e3e1e4",
    "sideBar.background": "#1e1f1c",
    "sideBarSectionHeader.background": "#2d2a2e",
    "menu.background": "#1e1f1c",
    "menu.foreground": "#cccccc",
    "pickerGroup.foreground": "#6f5e75",
    "input.background": "#403842",
    "inputOption.activeBorder": "#6f5e75",
    "focusBorder": "#6f5e75",
    "editorWidget.background": "#1e1f1c",
    "debugToolBar.background": "#1e1f1c",
    "diffEditor.insertedTextBackground": "#4b661680", // middle of #2d2a2e and #9ecd6f
    "diffEditor.removedTextBackground": "#90274A70", // middle of #2d2a2e and #f85e84
    "inputValidation.errorBackground": "#90274A", // middle of #2d2a2e and #f85e84
    "inputValidation.errorBorder": "#f85e84",
    "inputValidation.warningBackground": "#848528", // middle of #2d2a2e and #e2e22e
    "inputValidation.warningBorder": "#e2e22e",
    "inputValidation.infoBackground": "#546190", // middle of #2d2a2e and #819aff
    "inputValidation.infoBorder": "#819aff",
    "editorHoverWidget.background": "#403842",
    "editorHoverWidget.border": "#6f5e75",
    "editorSuggestWidget.background": "#2d2a2e",
    "editorSuggestWidget.border": "#6f5e75",
    "editorGroup.border": "#34352f",
    "peekView.border": "#6f5e75",
    "peekViewEditor.background": "#2d2a2e",
    "peekViewResult.background": "#1e1f1c",
    "peekViewTitle.background": "#1e1f1c",
    "peekViewResult.selectionBackground": "#403842",
    "peekViewResult.matchHighlightBackground": "#6f5e75",
    "peekViewEditor.matchHighlightBackground": "#6f5e75",
    "terminal.ansiBlack": "#333333",
    "terminal.ansiRed": "#C4265E", // the bright color with ~75% transparent on the background
    "terminal.ansiGreen": "#86B42B",
    "terminal.ansiYellow": "#B3B42B",
    "terminal.ansiBlue": "#6A7EC8",
    "terminal.ansiMagenta": "#8C6BC8",
    "terminal.ansiCyan": "#56ADBC",
    "terminal.ansiWhite": "#e3e3dd",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightRed": "#f85e84",
    "terminal.ansiBrightGreen": "#9ecd6f",
    "terminal.ansiBrightYellow": "#e2e22e", // hue shifted #9ecd6f
    "terminal.ansiBrightBlue": "#819aff", // hue shifted #ab9df2
    "terminal.ansiBrightMagenta": "#ab9df2",
    "terminal.ansiBrightCyan": "#7accd7",
    "terminal.ansiBrightWhite": "#e3e1e4"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#e3e1e4"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#e3e1e4"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#848089"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#e5c463"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#f85e84"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#e3e1e4"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ab9df2"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#ab9df2"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#ab9df2"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#e3e1e4"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#f85e84"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#f85e84"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7accd7"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ef9062"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#f85e84"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#7accd7"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#7accd7"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7accd7"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#F44747"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#6f5e75"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#f85e84"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e5c463"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#ab9df2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c463"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#f85e84"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#e5c463"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#7accd7"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#ef9062"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#9ecd6f"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#9ecd6f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6f5e75"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "#ab9df2"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "#e5c463"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "#e3e1e4"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#9ecd6f"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ef9062"
      }
    }
  ],
  "semanticHighlighting": true
}
